{ "browser": true, //browser constants, such as alert
  "curly": false, // don't {} on one-line if
  "newcap": true, // constructors must start with capital letter
  "jquery": true, // jQuery constants
  "nomen": false, // permit leading/trailing underscores, these do actually mean private in jQuery plugins
  "nonew": true, // don't permit object creation for side-effects only
  "asi": true, // don't require trailing semicolons, since js2-mode handles this already
  "strict": false, // don't require "use strict";
  "sub": true, // don't force foo['bar'] to be rewritten as foo.bar
  "unused": true, // warn on unused variables and parameters
  "undef": true, // warn on variables that aren't defined
  "regexp": true, // warn on unsafe . in regexps
  "maxerr": 10000, // keep reporting even if we find lots of issues
  "eqnull": true, // allow foo == null
  "-W099": true, // allowed mixed tabs and spaces
  "-W058": true, // allowed new Foo;
  "node": true, // node constants, such as require
  // whilst it's a bad idea to add "use strict"; at the top of a file
  // in the browser, it's good in node.
  "globalstrict": true,
  "laxcomma": true // allow comma-first style
}
